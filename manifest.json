{
  "name": "內容農場過濾器",
  "version": "1.0.0",
  "description": "在 google search 中封鎖內容農場",
  "manifest_version": 2,

  "content_scripts": [{
    "matches": ["https://*.google.com/*"],
    "js": ["script/watch.js"]
  }],

  "permissions": ["webRequest", "https://*.google.com/*", "http://*.google.com/*"],
  "web_accessible_resources": [
      "script/config.js"
    ]
}
